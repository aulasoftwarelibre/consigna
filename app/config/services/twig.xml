<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="consigna.extension.size" class="MovedBundle\Twig\TwigSizeExtension">
            <argument type="service" id="consigna.object_repository.file"/>
            <tag name="twig.extension"/>
        </service>

        <service id="twig.data_extension" class="Twig_Extensions_Extension_Date">
            <argument type="service" id="translator"/>
            <tag name="twig.extension"/>
        </service>

        <!--<service id="twig.text_extension" class="Twig_Extensions_Extension_Text">-->
            <!--<tag name="twig.extension"/>-->
        <!--</service>-->

    </services>
</container>