<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="consigna_file.manager.file" class="Bundle\FileBundle\Services\FileManager">
            <argument type="service" id="consigna_file.director.file"/>
            <argument type="service" id="consigna_file.event_dispatcher.file"/>
        </service>

        <service id="consigna.repository.file" alias="consigna_file.object_repository.file">
        </service>
    </services>
</container>