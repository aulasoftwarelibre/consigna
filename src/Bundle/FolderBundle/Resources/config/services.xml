<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="consigna_folder.manager.folder" class="Component\Folder\Services\FolderManager">
            <argument type="service" id="consigna_folder.director.folder"/>
            <argument type="service" id="consigna_folder.event_dispatcher.folder"/>
        </service>

        <service id="consigna.repository.folder" alias="consigna_folder.object_repository.folder">
        </service>
    </services>
</container>